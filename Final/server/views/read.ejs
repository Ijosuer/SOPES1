<!-- views/users.ejs -->
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/style.css">
    <title>Lista de Usuarios</title>
</head>
<body>
    <div class="container">
        <h2>Lista de Usuarios</h2>
        <ul>
            <% users.forEach(user => { %>
                <li><%= user.username %> - <%= user.password %></li>
            <% }); %>
        </ul>
    </div>
</body>
</html>
